{
    "version": 2,
    "builds": [
        {
            "src": "src/backend/src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/src/index.tsx"
        }
    ]
}